{
    "root": true,
    "parserOptions": {
        "requireConfigFile": false,
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "extends": "eslint:recommended",
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "globals": {
        "assert": "readonly",
        "describe": "readonly",
        "expect": "readonly",
        "it": "readonly",
        "jest": "readonly"
    },
    "overrides": [],
    "rules": {
        "array-bracket-newline": [1, "consistent"],
        "array-bracket-spacing": [
            1,
            "never",
            {
                "arraysInArrays": true,
                "objectsInArrays": false
            }
        ],
        "array-element-newline": [
            0,
            {
                "multiline": true,
                "minItems": 2
            }
        ],
        "arrow-parens": [2, "always"],
        "curly": [1, "multi-line"],
        "keyword-spacing": [
            2,
            {
                "before": true,
                "after": true,
                "overrides": {
                    "function": {
                        "after": false
                    },
                    "while": {
                        "after": false
                    }
                }
            }
        ],
        "indent": [2, 4],
        "linebreak-style": [2, "unix"],
        "max-len": 0,
        "no-extraneous-imports": 0,
        "no-prototype-builtins": 0,
        "no-unused-expressions": 0,
        "no-unused-vars": 1,
        "no-use-before-define": 0,
        "object-curly-newline": [
            1,
            {
                "consistent": true,
                "multiline": true,
                "minProperties": 2
            }
        ],
        "operator-linebreak": 0,
        "quotes": [2, "single"],
        "semi": [1, "always"],
        "space-before-function-paren": 0,
        "wrap-iife": [2, "inside"]
    }
}
